{
  "taskGroups": [ 
    {
      "taskSpec": {
        "runnables": [
          {
            "script": {
              "text": "echo task ${BATCH_TASK_INDEX} of ${BATCH_TASK_COUNT}"
            }
          }
        ],
        "computeResource": {
          "cpuMilli": 2000,
          "memoryMib": 16
        },
        "maxRetryCount": 2,
        "maxRunDuration": "3600s"
      },
      "taskCount": 4,
      "parallelism": 2
    }
  ],
  "allocationPolicy": {
    "instances": [
      {
        "policy": {"machineType": "e2-standard-4"}
      }
    ]
  },
  "labels": {
    "department": "finance",
    "env": "testing"
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
